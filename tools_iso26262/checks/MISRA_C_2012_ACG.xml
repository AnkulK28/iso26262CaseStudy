  <standard name="CUSTOM RULES">
    <section name="1 Files">
      <check id="1.1" state="off">
        <convention>File names should be lowercase and without spaces</convention>
        <pattern>[a-z][a-z0-9_]*.c</pattern>
        <comment># Violation when file name name uses uppercase letters or contains spaces</comment>
      </check>
      <check id="1.2" state="off">
      </check>
      <check id="1.3" state="off">
      </check>
      <check id="1.4" state="off">
      </check>
    </section>
    <section name="2 Preprocessing">
      <check id="2.1" state="off">
        <convention>Macros should be uppercased</convention>
        <pattern>[A-Z][A-Z0-9_]*</pattern>
        <comment># Violation when macros use lowercase letters</comment>
      </check>
      <check id="2.2" state="off">
      </check>
    </section>
    <section name="3 Typedefs">
      <check id="3.1" state="off">
        <convention>Typedefs should finish by "_t"</convention>
        <pattern>[A-Za-z0-9_]*_t</pattern>
        <comment># Violation when typedef statements do not end with "_t"</comment>
      </check>
      <check id="3.2" state="off">
        <convention>Typedefs should finish by "_t"</convention>
        <pattern>[A-Za-z0-9_]*_t</pattern>
        <comment># Violation when typedef statements do not end with "_t"</comment>
      </check>
      <check id="3.3" state="off">
        <convention>Typedefs should finish by "_t"</convention>
        <pattern>[A-Za-z0-9_]*_t</pattern>
        <comment># Violation when typedef statements do not end with "_t"</comment>
      </check>
      <check id="3.4" state="off">
        <convention>Typedefs should finish by "_t"</convention>
        <pattern>[A-Za-z0-9_]*_t</pattern>
        <comment># Violation when typedef statements do not end with "_t"</comment>
      </check>
      <check id="3.5" state="off">
        <convention>Typedefs should finish by "_t"</convention>
        <pattern>[A-Za-z0-9_]*_t</pattern>
        <comment># Violation when typedef statements do not end with "_t"</comment>
      </check>
    </section>
